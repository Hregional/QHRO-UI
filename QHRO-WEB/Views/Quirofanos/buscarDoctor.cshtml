
@{
    ViewBag.Title = "buscarDoctor";
}

<div class="container" style="margin-top:20px;">
    <
    <section>
        <table class="table table-dark">
            <thead>
                <tr>
                    <th scope="col">Nombre</th>
                    <th scope="col">Acciones</th>
                </tr>
            </thead>
            <tbody>
               
                @foreach (BLL.ClassDoctorsWeb item in ViewBag.doctors)
                {
                <tr>
                    <td>@item.DoctorName</td>
                    <td>
                        <button type="button" class="a btn btn-outline-success"><a  style="color: white; text-decoration:none"href="@Url.Action("agregarDoctor", "Quirofanos",new {idDoctor=item.IdDoctor , doctorName=item.DoctorName})">Agregar Doctor</a></button>
                        
                    </td>
                </tr>
                    
                }

            </tbody>
        </table>
    </section>
</div>
